# map home/end keys
"\e[H": beginning-of-line
"\e[F": end-of-line

# case sensitive matching is for chumps
set completion-ignore-case On

# expand ~ out to the full path
set expand-tilde on

# our terminals can always handle 8 bit
set input-meta on
set output-meta on

# show multiple completion matches
set show-all-if-ambiguous on
